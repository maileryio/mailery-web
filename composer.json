{
    "name": "maileryio/mailery-web",
    "type": "library",
    "description": "Mailery package for provide web components",
    "keywords": [
        "mail",
        "mailer",
        "web"
    ],
    "homepage": "https://github.com/maileryio/mailery-web",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/maileryio/mailery-web",
        "issues": "https://github.com/maileryio/mailery-web/issues",
        "wiki": "https://github.com/maileryio/mailery-web/wiki"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "foxy/foxy": "^1.0",
        "maileryio/mailery-brand": "dev-master",
        "maileryio/mailery-menu-navbar": "dev-master",
        "maileryio/mailery-menu-sidebar": "dev-master",
        "psr/http-factory": "^1.0",
        "yiisoft/aliases": "^2.0",
        "yiisoft/assets": "^1.0",
        "yiisoft/csrf": "^1.0",
        "yiisoft/data-response": "^1.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/http": "^1.1",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/session": "^1.0",
        "yiisoft/view": "^1.0",
        "yiisoft/widget": "^3.0@dev",
        "yiisoft/yii-bootstrap5": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/yii-view": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mailery\\Web\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailery\\Web\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "params.php",
            "web": "web.php"
        },
        "config-plugin-options": {
            "source-directory": "config"
        }
    },
    "config": {
        "sort-packages": true
    }
}
